{
  "presets": [
    "latest",
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel",
    [
      "react-css-modules",
      {
        "webpackHotModuleReloading": true,
        "generateScopedName": "[name]__[local]__[hash:base64:5]",
        "exclude": "/node_modules/",
        "removeImport": true,
        "filetypes": {
          ".scss": "postcss-scss"
        }
      }
    ],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
