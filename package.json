{
  "name": "cinemanio-frontend",
  "version": "0.0.1",
  "description": "Frontend for cineman.io",
  "repository": "https://github.com/cinemanio/frontend.git",
  "main": "index.js",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "test": "npm run jest && npm run flow && npm run lint",
    "jest": "jest --coverage .",
    "jest-watch": "jest --watch .",
    "flow": "flow check",
    "lint": "eslint --ext .js,.jsx .",
    "start": "npm run server-start & npm run client-start",
    "server-start": "nodemon --inspect",
    "client-start": "webpack-dev-server --config ./webpack.dev.config.js",
    "heroku-postbuild": "webpack -p --config ./webpack.prod.config.js --progress",
    "fetch-fragments-types": "babel-node ./lib/fragments/fetcher.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/tests/styleMock.js",
      "^((?:libs|stores|components|tests).*)$": "<rootDir>/$1"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "author": "ramusus <ramusus@gmail.com>",
  "license": "MIT",
