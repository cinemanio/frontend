{
  "name": "cinemanio-frontend",
  "version": "0.0.1",
  "description": "Frontend for cineman.io",
  "repository": "https://github.com/cinemanio/frontend.git",
  "main": "index.js",
  "engines": {
    "node": "11.6.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "start": "npm-run-all -p start:server start:client",
    "start:server": "nodemon --inspect -i cypress",
    "start:client": "webpack-dev-server --config ./webpack.dev.config.js",
    "start:prod": "node --harmony index.js",
    "build": "NODE_ENV=production webpack -p --config ./webpack.prod.config.js --json > stats.json",
    "test": "npm-run-all -s jest flow lint build size -c",
    "jest": "jest --coverage --maxWorkers=2 .",
    "jest:watch": "jest --watch .",
    "flow": "flow check",
    "lint": "npm-run-all -p eslint stylelint",
    "eslint": "eslint --ext .js,.jsx .",
    "stylelint": "stylelint **/*.scss",
    "size": "bundlesize",
    "prettier": "prettier --write '**/*.js*' !public/** !coverage/** !stats.json !flow-typed/**",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "heroku-postbuild": "npm run build",
    "fetch-fragments-types": "babel-node ./lib/fragments/fetcher.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(antd)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/tests/styleMock.js",
      "^((?:libs|stores|components|tests).*)$": "<rootDir>/$1"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "author": "ramusus <ramusus@gmail.com>",
  "license": "MIT",
  "bundlesize": [
    {
      "path": "./public/app.*.js",
      "maxSize": "533 kB"
    },
    {
      "path": "./public/app.*.css",
      "maxSize": "28 kB"
    }
  ],
